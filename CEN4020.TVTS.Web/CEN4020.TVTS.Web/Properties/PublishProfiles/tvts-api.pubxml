    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$tvts-api</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="TvtsDbEntities" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="server=us-cdbr-azure-east-b.cloudapp.net;user id=bdfdd1e81e01f8;password=a7820a0f;database=TvtsDb" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\TvtsDbEntities_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="metadata=res://*/TvtsDataModel.csdl|res://*/TvtsDataModel.ssdl|res://*/TvtsDataModel.msl;provider=MySql.Data.MySqlClient;provider connection string=&quot;server=us-cdbr-azure-east-b.cloudapp.net;user id=bdfdd1e81e01f8;password=a7820a0f;database=TvtsDb&quot;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="TvtsEntity" Order="2" Enabled="False">
