          <UpdateFrom Type="Web.Config">
            <Source MatchValue="metadata=res://*/TvtsEntityDataModel.csdl|res://*/TvtsEntityDataModel.ssdl|res://*/TvtsEntityDataModel.msl;provider=MySql.Data.MySqlClient;provider connection string=&quot;sslmode=Required;server=us-cdbr-azure-east-b.cloudapp.net;user id=bdfdd1e81e01f8;password=a7820a0f;database=TvtsDb&quot;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
